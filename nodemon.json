{
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "": "node --debug",
    "js": "node --debug"
  },
  "watch": [
    "server/app/",
    "server/bin/",
    "server/routes/",
    "server/views/",
    "server/middlewares/",
    "app.js"
  ],
  "ignore": [".git", ".idea", "node_modules"],
  "ext": "js jade"
}
